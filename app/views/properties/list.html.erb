<table>
  <tr>
    <th>list of all properties</th>
  </tr>
 
<% @properties.each do |property| %>
  <tr>
    <tr>
    <td><%= property.locality %></td>
    <td><%= property.city %></td>
    <td><%= property.state %></td>
    <td><%= property.country %></td>
    <td><%= property.rental_prize %></td>
    <td><%= property.selling_prize %></td>
    <td><%= link_to 'Show', property_path(property.id) %></td>
    
  </tr>
<% end %>
</table>
