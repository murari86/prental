<%= form_for :login url: search_properties_path do |f| %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email_id %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'signin', new_user_path %>
<%= link_to 'signup', new_registration_path %>
